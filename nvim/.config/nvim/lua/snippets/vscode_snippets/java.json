"Java IntegerPair": {
  "prefix": "intpair",
  "body": [
    "class Pair implements Comparable<Pair> {",
    "    public Integer i;",
    "    public Integer j;",
    "",
    "    public Pair(Integer i, Integer j) {",
    "        this.i = i;",
    "        this.j = j;",
    "    }",
    "",
    "    public Integer first() {",
    "        return this.i;",
    "    }",
    "",
    "    public Integer second() {",
    "        return this.j;",
    "    }",
    "",
    "    @Override",
    "    public String toString() {",
    "        return String.format(\"[%s, %s]\", this.i, this.j);",
    "    }",
    "",
    "    @Override",
    "    public int compareTo(Pair y) {",
    "        int x = this.i.compareTo(y.i);",
    "        return x != 0 ? x : this.j.compareTo(y.j);",
    "    }",
    "}"
  ],
  "description": "Java IntegerPair"
}
